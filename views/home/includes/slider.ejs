<div id="main-slide" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <% for (var i=0; i < slds.length; i++) { %>
            <li data-target="#main-slide" data-slide-to="<%= i %>" class="<%= slds[i].active %>">
            </li>
            <% } %>
    </ol>
    <div class="carousel-inner">
        <% for (var i=0; i < slds.length; i++) { %>
            <div class="carousel-item <%= slds[i].active %>"
                style="background-image: url(/images/slider/<%= slds[i].slider_image %>);">
                <div class="carousel-caption">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12">
                                <p class="fadeInUp wow" data-wow-delay=".6s">
                                    <%= slds[i].slider_txt1 %>
                                </p>
                                <h1 class="wow fadeInDown heading" data-wow-delay=".4s">
                                    <%= slds[i].slider_txt2 %>
                                </h1>
                                <a href="#pricing" class="fadeInLeft wow btn btn-common btn-lg" data-wow-delay=".6s">Get
                                    Ticket</a>
                                <a href="#gallery" class="fadeInRight wow btn btn-border btn-lg" data-wow-delay=".6s">Explore
                                    More</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <% } %>
    </div>
    <a class="carousel-control-prev" href="#main-slide" role="button" data-slide="prev">
        <span class="carousel-control" aria-hidden="true"><i class="lni-chevron-left"></i></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#main-slide" role="button" data-slide="next">
        <span class="carousel-control" aria-hidden="true"><i class="lni-chevron-right"></i></span>
        <span class="sr-only">Next</span>
    </a>
</div>